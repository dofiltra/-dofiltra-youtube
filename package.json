{
  "name": "@dofiltra/youtube",
  "version": "1.1.5",
  "description": "Search videos on YouTube without API key",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"jest --config jestconfig.json\"",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "pub": "yarn version --pre --patch && yarn run build && npm publish",
    "prepublishOnly": "yarn test && yarn run lint",
    "prepare": "yarn run build",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appit-online/youtube-search.git"
  },
  "keywords": [
    "youtube",
    "youtube search",
    "angular youtube search",
    "youtube api",
    "youtube crawler"
  ],
  "funding": {
    "url": "https://paypal.me/dave7117",
    "type": "paypal"
  },
  "author": "David Koller",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "bugs": {
    "url": "https://github.com/appit-online/youtube-search/issues"
  },
  "homepage": "https://github.com/appit-online/youtube-search#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "jest": "^27.4.4",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "got": "^11.8.3"
  }
}
